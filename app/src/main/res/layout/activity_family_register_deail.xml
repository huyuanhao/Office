<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white">

    <include
        android:id="@+id/layout_title"
        layout="@layout/common_title_bar" />

    <ScrollView
        android:id="@+id/sv_detail"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_above="@+id/ll_bottom"
        android:layout_below="@+id/layout_title">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <View
                android:layout_width="match_parent"
                android:layout_height="20px"
                android:background="@color/gray_system_bg" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="30px"
                android:layout_marginRight="30px"
                android:orientation="vertical">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">

                    <TextView
                        android:layout_width="374px"
                        android:layout_height="match_parent"
                        android:gravity="left|center"
                        android:paddingBottom="34px"
                        android:paddingLeft="25px"
                        android:paddingTop="34px"
                        android:text="@string/family_register_detail_item_name"
                        android:textColor="@color/color_333333"
                        android:textSize="44px" />

                    <View
                        android:layout_width="3px"
                        android:layout_height="match_parent"
                        android:background="@color/gray_system_bg" />

                    <TextView
                        android:id="@+id/tv_item_name"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:paddingBottom="34px"
                        android:paddingLeft="38px"
                        android:paddingRight="38px"
                        android:paddingTop="34px"
                        android:text=""
                        android:textColor="@color/common_gray"
                        android:textSize="44px" />
                </LinearLayout>

                <View
                    android:layout_width="match_parent"
                    android:layout_height="3px"
                    android:background="@color/gray_system_bg" />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/padding_10"
                    android:orientation="horizontal"
                    android:visibility="gone">

                    <TextView
                        style="@style/normal_item_name"
                        android:gravity="center"
                        android:text="@string/family_register_detail_work_object" />

                    <TextView
                        android:id="@+id/tv_work_object"
                        style="@style/normal_item_name"
                        android:text="" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">

                    <TextView
                        android:layout_width="374px"
                        android:layout_height="match_parent"
                        android:gravity="left|center"
                        android:paddingBottom="34px"
                        android:paddingLeft="25px"
                        android:paddingTop="34px"
                        android:text="@string/family_register_detail_actualize_department"
                        android:textColor="@color/color_333333"
                        android:textSize="44px" />

                    <View
                        android:layout_width="3px"
                        android:layout_height="match_parent"
                        android:background="@color/gray_system_bg" />

                    <TextView
                        android:id="@+id/tv_actualize_department"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:paddingBottom="34px"
                        android:paddingLeft="38px"
                        android:paddingRight="38px"
                        android:paddingTop="34px"
                        android:text=""
                        android:textColor="@color/common_gray"
                        android:textSize="44px" />
                </LinearLayout>

                <View
                    android:layout_width="match_parent"
                    android:layout_height="3px"
                    android:background="@color/gray_system_bg" />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">

                    <TextView
                        android:layout_width="374px"
                        android:layout_height="match_parent"
                        android:gravity="left|center"
                        android:paddingBottom="34px"
                        android:paddingLeft="25px"
                        android:paddingTop="34px"
                        android:text="@string/family_register_detail_transact_time_limit"
                        android:textColor="@color/color_333333"
                        android:textSize="44px" />

                    <View
                        android:layout_width="3px"
                        android:layout_height="match_parent"
                        android:background="@color/gray_system_bg" />

                    <TextView
                        android:id="@+id/tv_transact_time_limit"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:paddingBottom="34px"
                        android:paddingLeft="38px"
                        android:paddingRight="38px"
                        android:paddingTop="34px"
                        android:text=""
                        android:textColor="@color/common_gray"
                        android:textSize="44px" />

                </LinearLayout>

                <View
                    android:layout_width="match_parent"
                    android:layout_height="3px"
                    android:background="@color/gray_system_bg" />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">

                    <TextView
                        android:layout_width="374px"
                        android:layout_height="match_parent"
                        android:gravity="left|center"
                        android:paddingBottom="34px"
                        android:paddingLeft="25px"
                        android:paddingTop="34px"
                        android:text="@string/family_register_detail_fee_scale"
                        android:textColor="@color/color_333333"
                        android:textSize="44px" />

                    <View
                        android:layout_width="3px"
                        android:layout_height="match_parent"
                        android:background="@color/gray_system_bg" />

                    <TextView
                        android:id="@+id/tv_fee_scale"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:paddingBottom="34px"
                        android:paddingLeft="38px"
                        android:paddingRight="38px"
                        android:paddingTop="34px"
                        android:text=""
                        android:textColor="@color/common_gray"
                        android:textSize="44px" />
                </LinearLayout>

                <View
                    android:layout_width="match_parent"
                    android:layout_height="3px"
                    android:background="@color/gray_system_bg" />


                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">

                    <TextView
                        android:layout_width="374px"
                        android:layout_height="match_parent"
                        android:gravity="left|center"
                        android:paddingBottom="34px"
                        android:paddingLeft="25px"
                        android:paddingTop="34px"
                        android:text="@string/family_register_detail_degree"
                        android:textColor="@color/color_333333"
                        android:textSize="44px" />

                    <View
                        android:layout_width="3px"
                        android:layout_height="match_parent"
                        android:background="@color/gray_system_bg" />

                    <TextView
                        android:id="@+id/tv_degree"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:paddingBottom="34px"
                        android:paddingLeft="38px"
                        android:paddingRight="38px"
                        android:paddingTop="34px"
                        android:text=""
                        android:textColor="@color/common_gray"
                        android:textSize="44px" />

                </LinearLayout>

                <View
                    android:layout_width="match_parent"
                    android:layout_height="3px"
                    android:background="@color/gray_system_bg" />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">

                    <TextView
                        android:layout_width="374px"
                        android:layout_height="match_parent"
                        android:gravity="left|center"
                        android:paddingBottom="34px"
                        android:paddingLeft="25px"
                        android:paddingTop="34px"
                        android:text="@string/family_register_detail_refer_phone"
                        android:textColor="@color/color_333333"
                        android:textSize="44px" />

                    <View
                        android:layout_width="3px"
                        android:layout_height="match_parent"
                        android:background="@color/gray_system_bg" />

                    <TextView
                        android:id="@+id/tv_refer_phone"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:paddingBottom="34px"
                        android:paddingLeft="38px"
                        android:paddingRight="38px"
                        android:paddingTop="34px"
                        android:text=""
                        android:textColor="@color/common_gray"
                        android:textSize="44px" />

                </LinearLayout>

                <View
                    android:layout_width="match_parent"
                    android:layout_height="3px"
                    android:background="@color/gray_system_bg" />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">

                    <TextView
                        android:id="@+id/textView"
                        android:layout_width="374px"
                        android:layout_height="match_parent"
                        android:gravity="left|center"
                        android:paddingBottom="34px"
                        android:paddingLeft="25px"
                        android:paddingTop="34px"
                        android:text="@string/family_register_detail_monitor_phone"
                        android:textColor="@color/color_333333"
                        android:textSize="44px" />

                    <View
                        android:layout_width="3px"
                        android:layout_height="match_parent"
                        android:background="@color/gray_system_bg" />

                    <TextView
                        android:id="@+id/tv_monitor_phone"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:paddingBottom="34px"
                        android:paddingLeft="38px"
                        android:paddingRight="38px"
                        android:paddingTop="34px"
                        android:text=""
                        android:textColor="@color/common_gray"
                        android:textSize="44px" />

                </LinearLayout>

                <View
                    android:layout_width="match_parent"
                    android:layout_height="3px"
                    android:background="@color/gray_system_bg" />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">

                    <TextView
                        android:layout_width="374px"
                        android:layout_height="match_parent"
                        android:gravity="left|center"
                        android:paddingBottom="34px"
                        android:paddingLeft="25px"
                        android:paddingTop="34px"
                        android:text="@string/family_register_detail_transact_place"
                        android:textColor="@color/color_333333"
                        android:textSize="44px" />

                    <View
                        android:layout_width="3px"
                        android:layout_height="match_parent"
                        android:background="@color/gray_system_bg" />

                    <TextView
                        android:id="@+id/tv_transact_place"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:paddingBottom="34px"
                        android:paddingLeft="38px"
                        android:paddingRight="38px"
                        android:paddingTop="34px"
                        android:text=""
                        android:textColor="@color/common_gray"
                        android:textSize="44px" />
                </LinearLayout>

            </LinearLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="20px"
                android:background="@color/gray_system_bg" />

            <LinearLayout
                android:id="@+id/ll_bottom_tab"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">

                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:orientation="vertical">

                    <TextView
                        android:id="@+id/tv_sltj"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:clickable="true"
                        android:gravity="center"
                        android:paddingBottom="25px"
                        android:paddingTop="51px"
                        android:text="@string/accept_condition"
                        android:textColor="@color/common_gray"
                        android:textSize="46px" />

                    <View
                        android:id="@+id/view1"
                        android:layout_width="match_parent"
                        android:layout_height="8px"
                        android:layout_marginLeft="20px"
                        android:layout_marginRight="20px"
                        android:background="@color/main_select_color" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:orientation="vertical">

                    <TextView
                        android:id="@+id/tv_sxcl"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:clickable="true"
                        android:gravity="center"
                        android:paddingBottom="25px"
                        android:paddingTop="51px"
                        android:text="@string/transact_materials"
                        android:textColor="@color/common_gray"
                        android:textSize="46px" />

                    <View
                        android:id="@+id/view2"
                        android:layout_width="match_parent"
                        android:layout_height="8px"
                        android:layout_marginLeft="20px"
                        android:layout_marginRight="20px"
                        android:background="@color/main_select_color"
                        android:visibility="gone" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:orientation="vertical">

                    <TextView
                        android:id="@+id/tv_sfyj"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:clickable="true"
                        android:gravity="center"
                        android:paddingBottom="25px"
                        android:paddingTop="51px"
                        android:text="@string/charge_condition"
                        android:textColor="@color/common_gray"
                        android:textSize="46px" />

                    <View
                        android:id="@+id/view3"
                        android:layout_width="match_parent"
                        android:layout_height="8px"
                        android:layout_marginLeft="20px"
                        android:layout_marginRight="20px"
                        android:background="@color/main_select_color"
                        android:visibility="gone" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:orientation="vertical">

                    <TextView
                        android:id="@+id/tv_flyj"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:clickable="true"
                        android:gravity="center"
                        android:paddingBottom="25px"
                        android:paddingTop="51px"
                        android:text="@string/setting_gist"
                        android:textColor="@color/common_gray"
                        android:textSize="46px" />

                    <View
                        android:id="@+id/view4"
                        android:layout_width="match_parent"
                        android:layout_height="8px"
                        android:layout_marginLeft="20px"
                        android:layout_marginRight="20px"
                        android:background="@color/main_select_color"
                        android:visibility="gone" />
                </LinearLayout>


            </LinearLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="5px"
                android:background="@color/gray_system_bg" />
            <!-- 内容部分， fragment切换 -->

            <LinearLayout
                android:id="@+id/content_layout"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_margin="25px"
                android:orientation="vertical" />
        </LinearLayout>
    </ScrollView>

    <LinearLayout
        android:id="@+id/ll_bottom"
        android:layout_width="match_parent"
        android:layout_height="155px"
        android:layout_alignParentBottom="true"
        android:background="@color/blue_main"
        android:orientation="horizontal">

        <LinearLayout
            android:id="@+id/ll_online_booking"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:gravity="center"
            android:orientation="vertical">

            <TextView
                android:id="@+id/tv_online_booking"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:drawableLeft="@drawable/icon_guide_order_disabled"
                android:drawablePadding="20px"
                android:text="@string/online_booking"
                android:textColor="@color/hint"
                android:textSize="44px" />
        </LinearLayout>

        <View
            android:layout_width="1px"
            android:layout_height="match_parent"
            android:layout_marginBottom="30px"
            android:layout_marginTop="31px"
            android:background="@color/white" />

        <LinearLayout
            android:id="@+id/ll_online_declare"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:gravity="center"
            android:orientation="vertical">

            <TextView
                android:id="@+id/tv_online_declare"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:drawableLeft="@drawable/icon_guide_declare_disabled"
                android:drawablePadding="20px"
                android:text="@string/family_register_detail_online_declare"
                android:textColor="@color/hint"
                android:textSize="44px" />
        </LinearLayout>

        <View
            android:layout_width="1px"
            android:layout_height="match_parent"
            android:layout_marginBottom="30px"
            android:layout_marginTop="31px"
            android:background="@color/white" />

        <LinearLayout
            android:id="@+id/ll_route"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:gravity="center"
            android:orientation="vertical">

            <TextView
                android:id="@+id/route"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:drawableLeft="@drawable/icon_guide_route_disabled"
                android:drawablePadding="20px"
                android:text="@string/family_register_detail_route"
                android:textColor="@color/hint"
                android:textSize="44px" />
        </LinearLayout>

        <View
            android:layout_width="1px"
            android:layout_height="match_parent"
            android:layout_marginBottom="30px"
            android:layout_marginTop="31px"
            android:background="@color/white" />

        <LinearLayout
            android:id="@+id/ll_collect"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:gravity="center"
            android:orientation="vertical">

            <TextView
                android:id="@+id/tv_collect"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:drawableLeft="@drawable/icon_guide_collect"
                android:drawablePadding="20px"
                android:text="@string/family_register_detail_collect"
                android:textColor="@color/white"
                android:textSize="44px" />
        </LinearLayout>
    </LinearLayout>

</RelativeLayout>