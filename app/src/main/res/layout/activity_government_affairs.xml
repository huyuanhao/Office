<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/gray_split_line_bg"
    android:orientation="vertical">

    <include layout="@layout/common_title_bar" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

        <TextView
            android:id="@+id/tv_admin_power_list"
            style="@style/normal_item_name"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/dp15"
            android:layout_marginLeft="@dimen/dp15"
            android:layout_marginTop="@dimen/dp15"
            android:layout_weight="1"
            android:background="@drawable/sun_icon_b"
            android:drawableLeft="@drawable/sun_admin_power_list"
            android:gravity="center_vertical"
            android:padding="@dimen/dp10"
            android:paddingLeft="@dimen/dp5"
            android:text="@string/admin_power_list"
            android:textColor="@color/white"
            android:textSize="@dimen/ts14" />

        <TextView
            android:id="@+id/tv_department_duty_list"
            style="@style/normal_item_name"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_margin="@dimen/dp15"
            android:layout_weight="1"
            android:background="@drawable/sun_icon_g"
            android:drawableLeft="@drawable/sun_department_duty_list"
            android:gravity="center_vertical"
            android:padding="@dimen/dp10"
            android:paddingLeft="@dimen/dp5"
            android:text="@string/department_duty_list"
            android:textColor="@color/white"
            android:textSize="@dimen/ts14" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

        <TextView
            android:id="@+id/tv_admin_charge_list"
            style="@style/normal_item_name"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/dp15"
            android:layout_marginLeft="@dimen/dp15"
            android:layout_weight="1"
            android:background="@drawable/sun_icon_y"
            android:drawableLeft="@drawable/sun_admin_charge_list"
            android:gravity="center_vertical"
            android:padding="@dimen/dp10"
            android:paddingLeft="@dimen/dp5"
            android:text="@string/admin_charge_list"
            android:textColor="@color/white"
            android:textSize="@dimen/ts14" />

        <TextView
            android:id="@+id/tv_admin_approve_intermediary_services_list"
            style="@style/normal_item_name"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/dp15"
            android:layout_marginLeft="@dimen/dp15"
            android:layout_marginRight="@dimen/dp15"
            android:layout_weight="1"
            android:background="@drawable/sun_icon_o"
            android:drawableLeft="@drawable/sun_admin_approve_intermediary_services_list"
            android:gravity="center_vertical"
            android:padding="@dimen/dp10"
            android:paddingLeft="@dimen/dp5"
            android:text="@string/admin_approve_intermediary_services_list"
            android:textColor="@color/white"
            android:textSize="@dimen/ts14" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

        <TextView
            android:id="@+id/tv_market_admittance_list"
            style="@style/normal_item_name"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/dp15"
            android:layout_marginLeft="@dimen/dp15"
            android:layout_weight="1"
            android:background="@drawable/sun_icon_d"
            android:drawableLeft="@drawable/sun_market_admittance_list"
            android:gravity="center_vertical"
            android:padding="@dimen/dp10"
            android:paddingLeft="@dimen/dp5"
            android:text="@string/market_admittance_list"
            android:textColor="@color/white"
            android:textSize="@dimen/ts14" />

        <TextView
            android:id="@+id/tv_commonality_service_list"
            style="@style/normal_item_name"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/dp15"
            android:layout_marginLeft="@dimen/dp15"
            android:layout_marginRight="@dimen/dp15"
            android:layout_weight="1"
            android:background="@drawable/sun_icon_q"
            android:drawableLeft="@drawable/sun_commonality_service_list"
            android:gravity="center_vertical"
            android:padding="@dimen/dp10"
            android:paddingLeft="@dimen/dp5"
            android:text="@string/commonality_service_list"
            android:textColor="@color/white"
            android:textSize="@dimen/ts14"
            android:visibility="invisible" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/dp15"
        android:orientation="horizontal">

        <TextView
            android:id="@+id/tv_negative_publicity"
            style="@style/normal_item_name"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:drawablePadding="@dimen/dp10"
            android:drawableTop="@drawable/sun_publicity"
            android:gravity="center"
            android:text="@string/negative_publicity"
            android:textColor="@color/black"
            android:textSize="@dimen/ts14" />

        <View
            android:layout_width="1dp"
            android:layout_height="match_parent"
            android:layout_marginBottom="@dimen/dp20"
            android:layout_marginTop="@dimen/dp10"
            android:background="@color/gray_system_bg" />

        <TextView
            android:id="@+id/tv_policy_statute"
            style="@style/normal_item_name"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:drawablePadding="@dimen/dp10"
            android:drawableTop="@drawable/sun_law"
            android:gravity="center"
            android:text="@string/policy_statute"
            android:textColor="@color/black"
            android:textSize="@dimen/ts14" />

        <View
            android:layout_width="1dp"
            android:layout_height="match_parent"
            android:layout_marginBottom="@dimen/dp20"
            android:layout_marginTop="@dimen/dp10"
            android:background="@color/gray_system_bg" />

        <TextView
            android:id="@+id/tv_want_query"
            style="@style/normal_item_name"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:drawablePadding="@dimen/dp10"
            android:drawableTop="@drawable/sun_search"
            android:gravity="center"
            android:text="@string/want_query"
            android:textColor="@color/black"
            android:textSize="@dimen/ts14" />

        <View
            android:layout_width="1dp"
            android:layout_height="match_parent"
            android:layout_marginBottom="@dimen/dp20"
            android:layout_marginTop="@dimen/dp10"
            android:background="@color/gray_system_bg" />

        <TextView
            android:id="@+id/tv_query_statistics"
            style="@style/normal_item_name"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:drawablePadding="@dimen/dp10"
            android:drawableTop="@drawable/sun_query_statistics"
            android:gravity="center"
            android:text="@string/query_statistics"
            android:textColor="@color/black"
            android:textSize="@dimen/ts14" />
    </LinearLayout>

</LinearLayout>