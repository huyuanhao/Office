<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white">


    <include
        layout="@layout/common_title_bar"
        android:visibility="gone" />

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_above="@+id/ll_bottom"
        android:layout_marginBottom="@dimen/dp5">

        <LinearLayout

            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="@color/white"
            android:orientation="vertical">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/padding_10"
                android:layout_marginRight="@dimen/padding_10"
                android:layout_marginTop="@dimen/padding_10"
                android:orientation="horizontal">

                <TextView
                    style="@style/normal_item_name"
                    android:layout_width="0dp"
                    android:layout_marginRight="@dimen/padding_10"
                    android:layout_weight="1"
                    android:text="@string/bj_object_name"
                    android:textSize="@dimen/ts14" />

                <com.powerrich.office.oa.view.NoEmojiEditText
                    android:id="@+id/et_bj_object_name"
                    style="@style/normal_item_name"
                    android:layout_width="0dp"
                    android:layout_weight="2.2"
                    android:background="@drawable/gray_stroke_corners_bg"
                    android:inputType="text"
                    android:maxLines="3"
                    android:padding="@dimen/dp8"
                    android:singleLine="false"
                    android:textColor="@color/black"
                    android:textSize="@dimen/ts14" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/padding_10"
                android:layout_marginRight="@dimen/padding_10"
                android:layout_marginTop="@dimen/padding_10"
                android:orientation="horizontal">

                <TextView
                    style="@style/normal_item_name"
                    android:layout_width="0dp"
                    android:layout_marginRight="@dimen/padding_10"
                    android:layout_weight="1"
                    android:text="@string/bj_object_num"
                    android:textSize="@dimen/ts14" />

                <com.powerrich.office.oa.view.NoEmojiEditText
                    android:id="@+id/et_bj_object_num"
                    style="@style/normal_item_name"
                    android:layout_width="0dp"
                    android:layout_weight="2.2"
                    android:background="@drawable/gray_stroke_corners_bg"
                    android:inputType="text"
                    android:padding="@dimen/dp8"
                    android:textColor="@color/black"
                    android:textSize="@dimen/ts14"
                    android:visibility="gone" />

                <TextView
                    android:id="@+id/tv_bj_object_num"
                    style="@style/normal_item_name"
                    android:layout_width="0dp"
                    android:layout_weight="2.2"
                    android:background="@drawable/gray_stroke_corners_bg"
                    android:padding="@dimen/dp8"
                    android:textColor="@color/black"
                    android:textIsSelectable="true"
                    android:textSize="@dimen/ts14" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/padding_10"
                android:layout_marginRight="@dimen/padding_10"
                android:layout_marginTop="@dimen/padding_10"
                android:orientation="horizontal">

                <TextView
                    style="@style/normal_item_name"
                    android:layout_width="0dp"
                    android:layout_marginRight="@dimen/padding_10"
                    android:layout_weight="1"
                    android:text="@string/bj_event_name"
                    android:textSize="@dimen/ts14" />

                <com.powerrich.office.oa.view.NoEmojiEditText
                    android:id="@+id/et_bj_event_name"
                    style="@style/normal_item_name"
                    android:layout_width="0dp"
                    android:layout_weight="2.2"
                    android:background="@drawable/gray_stroke_corners_bg"
                    android:inputType="text"
                    android:maxLines="3"
                    android:padding="@dimen/dp8"
                    android:singleLine="false"
                    android:textColor="@color/black"
                    android:textSize="@dimen/ts14" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/padding_10"
                android:layout_marginRight="@dimen/padding_10"
                android:layout_marginTop="@dimen/padding_10"
                android:orientation="horizontal">

                <TextView
                    style="@style/normal_item_name"
                    android:layout_width="0dp"
                    android:layout_marginRight="@dimen/padding_10"
                    android:layout_weight="1"
                    android:text="@string/bj_event_num"
                    android:textIsSelectable="true"
                    android:textSize="@dimen/ts14" />

                <com.powerrich.office.oa.view.NoEmojiEditText
                    android:id="@+id/et_bj_event_num"
                    style="@style/normal_item_name"
                    android:layout_width="0dp"
                    android:layout_weight="2.2"
                    android:background="@drawable/gray_stroke_corners_bg"
                    android:inputType="text"
                    android:padding="@dimen/dp8"
                    android:textColor="@color/black"
                    android:textSize="@dimen/ts14" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/padding_10"
                android:layout_marginRight="@dimen/padding_10"
                android:layout_marginTop="@dimen/padding_10"
                android:orientation="horizontal">

                <TextView
                    style="@style/normal_item_name"
                    android:layout_width="0dp"
                    android:layout_marginRight="@dimen/padding_10"
                    android:layout_weight="1"
                    android:text="@string/bj_apply_time"
                    android:textSize="@dimen/ts14" />

                <com.powerrich.office.oa.view.NoEmojiEditText
                    android:id="@+id/et_bj_apply_time"
                    style="@style/normal_item_name"
                    android:layout_width="0dp"
                    android:layout_weight="2.2"
                    android:background="@drawable/gray_stroke_corners_bg"
                    android:inputType="text"
                    android:padding="@dimen/dp8"
                    android:textColor="@color/black"
                    android:textSize="@dimen/ts14" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/padding_10"
                android:layout_marginRight="@dimen/padding_10"
                android:layout_marginTop="@dimen/padding_10"
                android:orientation="horizontal">

                <TextView
                    style="@style/normal_item_name"
                    android:layout_width="0dp"
                    android:layout_marginRight="@dimen/padding_10"
                    android:layout_weight="1"
                    android:text="@string/bj_apply_person_name"
                    android:textSize="@dimen/ts14" />

                <com.powerrich.office.oa.view.NoEmojiEditText
                    android:id="@+id/et_bj_apply_person_name"
                    style="@style/normal_item_name"
                    android:layout_width="0dp"
                    android:layout_weight="2.2"
                    android:background="@drawable/gray_stroke_corners_bg"
                    android:inputType="text"
                    android:maxLines="3"
                    android:padding="@dimen/dp8"
                    android:singleLine="false"
                    android:textColor="@color/black"
                    android:textSize="@dimen/ts14" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/padding_10"
                android:layout_marginRight="@dimen/padding_10"
                android:layout_marginTop="@dimen/padding_10"
                android:orientation="horizontal">

                <TextView
                    style="@style/normal_item_name"
                    android:layout_width="0dp"
                    android:layout_marginRight="@dimen/padding_10"
                    android:layout_weight="1"
                    android:text="@string/bj_linkman_id_type"
                    android:textSize="@dimen/ts14" />

                <com.powerrich.office.oa.view.NoEmojiEditText
                    android:id="@+id/et_bj_linkman_id_type"
                    style="@style/normal_item_name"
                    android:layout_width="0dp"
                    android:layout_weight="2.2"
                    android:background="@drawable/gray_stroke_corners_bg"
                    android:inputType="text"
                    android:padding="@dimen/dp8"
                    android:textColor="@color/black"
                    android:textSize="@dimen/ts14" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/padding_10"
                android:layout_marginRight="@dimen/padding_10"
                android:layout_marginTop="@dimen/padding_10"
                android:orientation="horizontal">

                <TextView
                    style="@style/normal_item_name"
                    android:layout_width="0dp"
                    android:layout_marginRight="@dimen/padding_10"
                    android:layout_weight="1"
                    android:text="@string/bj_linkman_id_num"
                    android:textSize="@dimen/ts14" />

                <com.powerrich.office.oa.view.NoEmojiEditText
                    android:id="@+id/et_bj_linkman_id_num"
                    style="@style/normal_item_name"
                    android:layout_width="0dp"
                    android:layout_weight="2.2"
                    android:background="@drawable/gray_stroke_corners_bg"
                    android:inputType="text"
                    android:padding="@dimen/dp8"
                    android:textColor="@color/black"
                    android:textSize="@dimen/ts14" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/padding_10"
                android:layout_marginRight="@dimen/padding_10"
                android:layout_marginTop="@dimen/padding_10"
                android:orientation="horizontal">

                <TextView
                    style="@style/normal_item_name"
                    android:layout_width="0dp"
                    android:layout_marginRight="@dimen/padding_10"
                    android:layout_weight="1"
                    android:text="@string/bj_linkman_name"
                    android:textSize="@dimen/ts14" />

                <com.powerrich.office.oa.view.NoEmojiEditText
                    android:id="@+id/et_bj_linkman_name"
                    style="@style/normal_item_name"
                    android:layout_width="0dp"
                    android:layout_weight="2.2"
                    android:background="@drawable/gray_stroke_corners_bg"
                    android:inputType="text"
                    android:padding="@dimen/dp8"
                    android:textColor="@color/black"
                    android:textSize="@dimen/ts14" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/padding_10"
                android:layout_marginRight="@dimen/padding_10"
                android:layout_marginTop="@dimen/padding_10"
                android:orientation="horizontal">

                <TextView
                    style="@style/normal_item_name"
                    android:layout_width="0dp"
                    android:layout_marginRight="@dimen/padding_10"
                    android:layout_weight="1"
                    android:text="@string/bj_linkman_mobile_phone"
                    android:textSize="@dimen/ts14" />

                <com.powerrich.office.oa.view.NoEmojiEditText
                    android:id="@+id/et_bj_linkman_mobile_phone"
                    style="@style/normal_item_name"
                    android:layout_width="0dp"
                    android:layout_weight="2.2"
                    android:background="@drawable/gray_stroke_corners_bg"
                    android:inputType="text"
                    android:padding="@dimen/dp8"
                    android:textColor="@color/black"
                    android:textSize="@dimen/ts14" />
            </LinearLayout>


            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/padding_10"
                android:layout_marginRight="@dimen/padding_10"
                android:layout_marginTop="@dimen/padding_10"
                android:orientation="horizontal">

                <TextView
                    style="@style/normal_item_name"
                    android:layout_width="0dp"
                    android:layout_marginRight="@dimen/padding_10"
                    android:layout_weight="1"
                    android:text="@string/bj_link_address"
                    android:textSize="@dimen/ts14" />

                <com.powerrich.office.oa.view.NoEmojiEditText
                    android:id="@+id/et_bj_link_address"
                    style="@style/normal_item_name"
                    android:layout_width="0dp"
                    android:layout_weight="2.2"
                    android:background="@drawable/gray_stroke_corners_bg"
                    android:gravity="left|top"
                    android:inputType="text|textMultiLine"
                    android:lines="3"
                    android:padding="@dimen/dp8"
                    android:textColor="@color/black"
                    android:textSize="@dimen/ts14" />
            </LinearLayout>

        </LinearLayout>
    </ScrollView>

    <LinearLayout
        android:id="@+id/ll_bottom"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:orientation="horizontal">

        <TextView
            android:id="@+id/tv_see_form"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:background="@color/blue_main"
            android:gravity="center"
            android:padding="@dimen/dp10"
            android:text="查看表单"
            android:textColor="@color/white"
            android:textSize="@dimen/ts16"
            android:visibility="gone" />

        <View
            android:layout_width="@dimen/dp1"
            android:layout_height="match_parent"
            android:background="@color/white"
            android:visibility="gone" />

        <TextView
            android:id="@+id/tv_see_file"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:background="@color/blue_main"
            android:gravity="center"
            android:padding="@dimen/dp10"
            android:text="查看附件"
            android:textColor="@color/white"
            android:textSize="@dimen/ts16"
            android:visibility="gone" />

        <View
            android:id="@+id/v_continue"
            android:layout_width="@dimen/dp1"
            android:layout_height="match_parent"
            android:background="@color/white"
            android:visibility="gone" />

        <TextView
            android:id="@+id/tv_continue"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:background="@color/blue_main"
            android:gravity="center"
            android:padding="@dimen/dp10"
            android:text="继续办理"
            android:textColor="@color/white"
            android:textSize="@dimen/ts16" />

        <View
            android:id="@+id/v_redo"
            android:layout_width="@dimen/dp1"
            android:layout_height="match_parent"
            android:background="@color/white"
            android:visibility="gone" />

        <TextView
            android:id="@+id/tv_redo"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:background="@color/blue_main"
            android:gravity="center"
            android:padding="@dimen/dp10"
            android:text="重新办理"
            android:textColor="@color/white"
            android:textSize="@dimen/ts16"
            android:visibility="gone" />


    </LinearLayout>
</RelativeLayout>