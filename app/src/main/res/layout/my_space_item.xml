<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:paddingLeft="15dp"
    android:paddingTop="12sp"
    android:paddingRight="12sp"
    android:paddingBottom="8dp"

    android:orientation="vertical">



    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"

        android:orientation="horizontal">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            style="@style/text_my_space"
            android:text="证照名称：" />

        <TextView
            android:id="@+id/tv_zjmc_value"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            style="@style/text_my_space"
            tools:text="营业执照（A类优先责任）" />
    </LinearLayout>

    <LinearLayout
        android:layout_marginTop="8dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"

        android:orientation="horizontal">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            style="@style/text_my_space"
            android:text="证照号码：" />

        <TextView
            android:id="@+id/tv_zjhm_value"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            style="@style/text_my_space"
            tools:text="营业执照（A类优先责任）" />
    </LinearLayout>

    <LinearLayout
        android:layout_marginTop="8dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            style="@style/text_my_space"
            android:text="持有人：" />

        <TextView
            android:id="@+id/tv_jyzz_value"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            style="@style/text_my_space"
            tools:text="营业执照（A类优先责任）" />
    </LinearLayout>

    <LinearLayout
        android:layout_marginTop="8dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            style="@style/text_my_space"
            android:text="发证机关：" />

        <TextView
            android:id="@+id/tv_fzjg_value"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            style="@style/text_my_space"
            tools:text="营业执照（A类优先责任）" />
    </LinearLayout>

    <LinearLayout
        android:layout_marginTop="8dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"

        android:orientation="horizontal">

        <TextView

            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            style="@style/text_my_space"
            android:text="签发日期：" />

        <TextView
            android:id="@+id/tv_qfrq_value"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            style="@style/text_my_space"
            tools:text="营业执照（A类优先责任）" />
    </LinearLayout>

</LinearLayout>
