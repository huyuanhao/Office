<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape>
            <corners android:radius="10dp" />
            <solid android:color="#e3e4e5" />          //这个颜色是progressbar的背景颜色
        </shape>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <corners android:radius="10dp" />
                <solid android:color="@color/common_top_titlebar_bgcolor" />       //这个颜色是progressbar的进度颜色

            </shape>
        </clip>
    </item>
</layer-list>